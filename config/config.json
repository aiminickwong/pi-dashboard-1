{
    "update_interval": 5000,
    "hosts": [
        "homescreen",
        "backup-pi"
    ],
    "server": {
        "protocol": "http",
        "port": 9630
    },
    "excluded_disks": [
        "/dev",
        "/dev/shm",
        "/run",
        "/run/lock",
        "/sys/fs/cgroup",
        "/run/user/1000"
    ],
    "debugEnabled": true
}